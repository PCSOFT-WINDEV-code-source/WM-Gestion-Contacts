      // Résumé : Remplit la COMBO des numéros
      // Syntaxe :
      //RemplitNumérosSelonContact ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      
      Procédure RemplitNumérosSelonContact()
      
      // Supprime les anciens numéros
      ListeSupprimeTout(COMBO_Numéros)
      
      pour tout tTéléphone de gContact.Téléphone
      	listeajoute(COMBO_Numéros,tTéléphone..Numéro)
      FIN
      
      SI COMBO_Numéros..Occurrence > 0 ALORS
      	COMBO_Numéros = 1
      SINON
      	Erreur("Ce contact n'a pas de numéro de téléphone.")
      	ferme()
      FIN
      
