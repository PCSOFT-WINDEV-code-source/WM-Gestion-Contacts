         SI COMBO_Numéros = "" ALORS
         	Erreur("Vous devez saisir un numéro de téléphone.")
         	retour
         	
         SINON
         	TitreSuivant("Envoi du SMS en cours...")
         	
         	//Remise à 0 de tous les membres de la structure Email
         	SMSRAZ()
         	
         	SMS.Message = SAI_MESSAGE
         	SMS.Numéro = COMBO_Numéros[COMBO_Numéros]
         	
         	//Envoi du SMS
         
         <SI CibleExécution=Android>
         		
         		SI PAS SMSEnvoie() ALORS
         			Sablier(Faux)
         			Erreur("Cet SMS n'a pas pu être envoyé.")
         		SINON
         			Sablier(Faux)
         			ToastAffiche("Le SMS a bien été envoyé.")
         			Ferme()
         		FIN
         	
         <SINON SI CibleExécution=iOS>
         	
         	
         	SMSLanceAppli()
         	ferme()
         	
         <FIN>
         
         	
         	
         	
         	
         FIN
         
